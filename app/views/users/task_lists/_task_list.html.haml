- div_for task_list do
  %h3= link_to task_list.title, [@user, task_list]
  = image_tag "private.png" if task_list.private?
  %br
  = link_to "Edit", edit_user_task_list_path(@user, task_list)
  = link_to "Destroy", user_task_list_path(@user, task_list), :confirm => "Are you sure?", :method => :delete
